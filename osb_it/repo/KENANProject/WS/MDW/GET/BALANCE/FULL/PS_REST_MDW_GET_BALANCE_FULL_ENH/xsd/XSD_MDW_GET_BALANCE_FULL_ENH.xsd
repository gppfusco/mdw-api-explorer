<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.skytv.it/mdw/data" elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.skytv.it/mdw/data">
  <xsd:complexType name="requestGetBalanceFullType">
    <xsd:sequence>
      <xsd:element name="DataOra" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="IdUtenza" type="xsd:string" maxOccurs="1"/>
      <xsd:element name="NumeroFatture" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Contesto" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="FlgUltimaFattura" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="FlgChangeMop" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="FlgPayAll" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="ComponentType" type="ComponentTypeDef" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="FlgScaduto" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="responseGetBalanceFullType">
    <xsd:sequence>
      <xsd:element name="esito" type="xsd:string" maxOccurs="1"/>
      <xsd:element name="descrizioneEsito" type="xsd:string" maxOccurs="1"/>
      <xsd:element name="Result" minOccurs="0">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="BillingDetails" type="BillingDetailsType" minOccurs="0"/>
            <xsd:element name="ImportoTotale" type="xsd:string" minOccurs="0"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BillingDetailsType">
    <xsd:sequence>
      <xsd:element name="BillingDetail" type="BillingDetailType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BillingDetailType">
    <xsd:sequence>
      <xsd:element name="CodiceFiscale" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="NomeIntestatario" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="CodiceContratto" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="StatoContratto" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ImportoVariabile" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ImportoPenale" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ImportoCanoni" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ImportoPenaliApparati" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="SpeseRecuperoCrediti" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Sconti" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ImportoScaduto" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ImportoAScadere" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ImportoBalance" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Note" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ComponentType" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="listaFatture" type="listaFattureType2" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="listaFattureType2">
    <xsd:sequence>
      <xsd:element name="fattura" minOccurs="0" maxOccurs="unbounded">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="DataScadenzaFattura" type="xsd:string" minOccurs="0"/>
            <xsd:element name="DataEmissioneFattura" type="xsd:string" minOccurs="0"/>
            <xsd:element name="NumeroFattura" type="xsd:string" minOccurs="0"/>
            <xsd:element name="importoFatturaDovuto" type="xsd:string" minOccurs="0"/>
            <xsd:element name="importoTotaleFattura" type="xsd:string" minOccurs="0"/>
            <xsd:element name="statoControlloFattura" type="xsd:string" minOccurs="0"/>
            <xsd:element name="provenienzaControlloFattura" type="xsd:string" minOccurs="0"/>
            <xsd:element name="stato" type="xsd:string" minOccurs="0"/>
            <xsd:element name="codiceMigrazione" type="xsd:string" minOccurs="0"/>
            <xsd:element name="indirizzoFatturazione" type="xsd:string" minOccurs="0"/>
            <xsd:element name="beniMateriali" type="beniMaterialiType" minOccurs="0"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="beniMaterialiType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BENI TV"/>
      <xsd:enumeration value="FATTURA NORMALE TV"/>
      <xsd:enumeration value="BENI BB"/>
      <xsd:enumeration value="FATTURA NORMALE BB"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ComponentTypeDef">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TV"/>
      <xsd:enumeration value="BB"/>
      <xsd:enumeration value="BB+TV"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="statoControlloFatturaType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="FATTURA PRODOTTA"/>
      <xsd:enumeration value="FATTURA SOPPRESSA"/>
      <xsd:enumeration value="OK"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="requestGetBalanceFull" type="requestGetBalanceFullType"/>
  <xsd:element name="responseGetBalanceFull" type="responseGetBalanceFullType"/>
</xsd:schema>